	<!ELEMENT USERSLOT_TRANSLATE_HORIZONTAL EMPTY >
	<!ATTLIST USERSLOT_TRANSLATE_HORIZONTAL key (Joy_X|Joy_X_p|Joy_X_n|Joy_Y|Joy_Y_p|Joy_Y_n|Joy_x|Joy_x_p|Joy_x_n|Joy_y|Joy_y_p|Joy_y_n|Joy_Ovr) #REQUIRED>

