	<!ELEMENT USERSLOT_HYD_ENABLE EMPTY >
	<!ATTLIST USERSLOT_HYD_ENABLE key (Joy_DC_D|Joy_DC_U|Joy_DC_L|Joy_DC_R|Joy_BT_UL|Joy_BT_UM|Joy_BT_UR|Joy_BT_DL|Joy_BT_DR|Joy_TG|Joy_SW_U|Joy_SW_D) #REQUIRED>

