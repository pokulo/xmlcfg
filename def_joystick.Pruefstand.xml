<joystick_config>
	/// menu functions
<USERSLOT_MENU_ENTER key="Joy_BT_DL"/>
<USERSLOT_MENU_ESCAPE key="Joy_BT_DR"/>
<USERSLOT_MENU_UP key="Joy_DC_U"/>
<USERSLOT_MENU_DOWN key="Joy_DC_D"/>
<USERSLOT_MENU_LEFT key="Joy_DC_L"/>
<USERSLOT_MENU_RIGHT key="Joy_DC_R"/>

/// hydraulic and safety functions
<USERSLOT_HYD_ENABLE key="Joy_SW_U"/>
<USERSLOT_HYD_PRESSURE key="Joy_SW_D"/>

/// degrees of freedom working space
<USERSLOT_TRANSLATE_HORIZONTAL key="Joy_X"/>
<USERSLOT_TRANSLATE_VERTICAL key="Joy_Y"/>
<USERSLOT_TRANSLATE_LONGITUDINAL key="Joy_Ovr"/>

<USERSLOT_ROTATE_HORIZONTAL key="Joy_x"/>
<USERSLOT_ROTATE_VERTICAL key="Joy_y"/>
<USERSLOT_ROTATE_LONGITUDINAL key="Joy_Ovr"/>

/// degrees of freedom in single axis space
<USERSLOT_SINGLE_POSITION key="Joy_x"/>
<USERSLOT_SINGLE_VELOCITY key="Joy_y"/>
<USERSLOT_SINGLE_VALVE_VAL key="Joy_Ovr"/>

</joystick_config>
